    }

    .home_section .navbar .nav-item {
        text-transform: uppercase;
        padding: 10px;
        align-items: center;
    }

        .home_section .navbar .nav-item .nav-link {
            position: relative;
        }

            .home_section .navbar .nav-item .nav-link::after {
                content: "";
                opacity: 0;
                transition: all 0.4s;
                height: 2px;
                width: 0;
                background-color: #165a4a;
                position: absolute;
                bottom: 0;
                left: 0;
            }

            .home_section .navbar .nav-item .nav-link:hover::after {
                opacity: 1;
                width: 100%;
            }

            .home_section .navbar .nav-item .nav-link:hover {
                color: #165a4a;
            }

    .home_section .navbar .btn {
        width: -moz-max-content;
        width: max-content;
        margin: 20px;
        border-radius: 5px;
        border: 2px solid #165a4a;
        background-color: #165a4a;
        color: #eaf1f0;
        text-transform: uppercase;
    }

        .home_section .navbar .btn:hover {
            border: 2px solid #165a4a;
            color: #165a4a;
            background-color: white;
        }

    .carousel {
        width: 100%;
        height: 100vh;
        overflow: hidden;
        position: relative;
    }

        .carousel .list .item {
            width: 100%;
            height: 100%;
            position: absolute;
            inset: 0 0 0 0;
        }

            .carousel .list .item img {
                width: 100%;
                height: 100%;
                -o-object-fit: cover;
                object-fit: cover;
            }

            .carousel .list .item .content {
                position: absolute;
                top: 20%;
                max-width: 80%;
                left: 12%;
                transform: translateX(-10%);
                box-sizing: border-box;
                color: #eaf1f0;
                text-shadow: 0 10px 10px rgba(0, 0, 0, 0.2666666667);
            }

                .carousel .list .item .content .display-4 {
                    margin: 15px auto;
                }

                .carousel .list .item .content .get-estimate-btn {
                    border: 2px solid #165a4a;
                    background-color: #165a4a;
                    color: white;
                    padding: 10px;
                    font-weight: 500;
                    font-size: 18px;
                    cursor: pointer;
                    letter-spacing: 1px;
                    transition: 0.4s;
                    margin: 15px auto;
                    border-radius: 5px;
                    text-transform: uppercase;
                }

                    .carousel .list .item .content .get-estimate-btn:hover {
                        background-color: transparent;
                        -webkit-backdrop-filter: blur(2px);
                        backdrop-filter: blur(2px);
                        letter-spacing: 2px;
                    }

    .thumbnail {
        display: flex;
        gap: 20px;
        position: absolute;
        bottom: 20px;
        left: 30%;
        max-width: -moz-max-content;
        max-width: max-content;
        z-index: 100;
    }

        .thumbnail .item {
            width: 150px;
            height: 220px;
            flex-shrink: 0;
            position: relative;
        }

            .thumbnail .item img {
                width: 100%;
                height: 100%;
                -o-object-fit: cover;
                object-fit: cover;
                border-radius: 20px;
                box-shadow: 5px 0 15px rgba(0, 0, 0, 0.3);
            }

    .nextPrevArrows {
        position: absolute;
        top: 80%;
        right: 60%;
        z-index: 100;
        width: 300px;
        max-width: 30%;
        display: flex;
        gap: 10px;
        align-items: center;
    }

        .nextPrevArrows button {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            background-color: #165a4a;
            border: solid #165a4a;
            color: #fff;
            font-family: monospace;
            font-weight: bold;
            cursor: pointer;
            transition: 0.5s;
        }

            .nextPrevArrows button:hover {
                background-color: transparent;
                border: solid #165a4a;
            }

    .carousel .list .item:nth-child(1) {
        z-index: 1;
    }

        .carousel .list .item:nth-child(1) .content {
            transform: translateY(50px);
            filter: blur(20px);
            opacity: 0;
            animation: showContent 0.5s 1s linear 1 forwards;
        }

    @@keyframes showContent {
        to {
            opacity: 1;
            filter: blur(0);
            transform: translateY(0px);
        }
    }

    .carousel .list .item:nth-child(1) .display-4 {
        animation-delay: 0.8s;
    }

    .carousel .list .item:nth-child(1) .fs-4 {
        animation-delay: 1s;
    }

    .carousel .list .item:nth-child(1),
    .get-estimate-btn {
        animation-delay: 1.5s;
    }

    .carousel.next .list .item:nth-child(1) img {
        width: 150px;
        height: 220px;
        position: absolute;
        bottom: 50px;
        left: 50%;
        border-radius: 20px;
        animation: showImage 0.5s linear 1 forwards;
    }

    @@keyframes showImage {
        to {
            width: 100%;
            height: 100%;
            bottom: 0;
            left: 0;
            border-radius: 0;
        }
    }

    .carousel.next .thumbnail .item:nth-child(1) {
        width: 0;
        overflow: hidden;
        animation: showThumbnail 0.5s linear forwards;
    }

    @@keyframes showThumbnail {
        to {
            width: 150px;
        }
    }

    .carousel.next .thumbnail {
        transform: translateX(150px);
        animation: transformThumbnail 0.5s linear 1 forwards;
    }

    @@keyframes transformThumbnail {
        to {
            transform: translateX(0);
        }
    }

    .carousel.prev .list .item:nth-child(2) {
        z-index: 2;
    }

        .carousel.prev .list .item:nth-child(2) img {
            position: absolute;
            bottom: 0;
            left: 0;
            animation: outImage 0.5s linear 1 forwards;
        }

    @@keyframes outImage {
        to {
            width: 150px;
            height: 220px;
            border-radius: 20px;
            left: 30%;
            bottom: 20px;
        }
    }

    .carousel.prev .thumbnail .item:nth-child(1) {
        width: 0;
        overflow: hidden;
        opacity: 0;
        animation: showThumbnail 0.5s linear 1 forwards;
    }

    .carousel.prev .list .item:nth-child(2) .content {
        animation: contentOut 0.5s 1s linear 1 forwards;
    }

    @@keyframes contentOut {
        to {
            transform: translateY(-150px);
            filter: blur(20px);
            opacity: 0;
        }
    }

    .carousel.next .nextPrevArrows button,
    .carousel.prev .nextPrevArrows button {
        pointer-events: none;
    }

    .time {
        width: 0%;
        height: 5px;
        background-color: #165a4a;
        position: absolute;
        z-index: 100;
        top: 0;
        left: 0;
    }

    .carousel.next .time,
    .carousel.prev .time {
        width: 100%;
        animation: timeRunning 2s linear 1 forwards;
    }

    @@keyframes timeRunning {
        to {
            width: 0;
        }
    }

